<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:width="1px" android:color="@color/baselib_orange" />
            <corners android:radius="10dp" />
            <solid android:color="@color/baselib_green" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="1px" android:color="@color/baselib_red" />
            <corners android:radius="10dp" />
            <solid android:color="@color/baselib_blue" />
        </shape>
    </item>
</selector>